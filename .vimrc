syntax on
set enc=utf-8
set nu
set tabstop=2
set expandtab
set autoindent
set laststatus=3
set t_Co=256
set noundofile
set nobackup
setlocal noswapfile
set showmode
set showcmd
set shortmess=ati
set noerrorbells
